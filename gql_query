query TournamentQuery($slug: String) {
  
  	
		tournament(slug: $slug){
			id
			name
			events {
				id
				name
        phaseGroups {
          id
        }
			}
		}
  phaseGroup( id : 852701)	{
    sets {
      id
      games {
        selections {
          selectionType
          selectionValue
        }
        id
      }
    }
  }
}
